! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #212121
*.color0:     #1976D2
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

! urxvt
URxvt.iconFile:              /usr/share/icons/Papirus/64x64/apps/utilities-terminal.svg
urxvt.scrollBar:             false
urxvt.depth:                 32
urxvt.internalBorder:        10
urxvt.font:                  xft:Hasklug Nerd Font:pixelsize=14,xft:Symbola:pixelsize:15
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C
URxvt*termName:              rxvt-unicode
!URxvt.keysym.Home:           \033[1~
!URxvt.keysym.End:            \033[4~
!URxvt.keysym.KP_Home:        \033[1~
!URxvt.keysym.KP_End:         \033[4~

! rofi

rofi.location:          0
rofi.width:             55
rofi.padding:           30
rofi.eh:                1
rofi.bw:                0
rofi.lines:             10
rofi.font:              Roboto Slab 13
rofi.line-margin:       5
rofi.columns:           3
rofi.hide-scrollbar:    true
rofi.sidebar-mode:      true
rofi.modi:              drun,run
rofi.show-icons:        true
rofi.kb-mode-next:      Tab
rofi.kb-row-tab:

! State:                'bg',     'fg',      'bgalt',   'hlbg',   'hlfg'
rofi.color-normal: #00000000,  #ffffff,    #00000000,  #e91e63,  #ffffff
rofi.color-urgent: #00000000,  #dc322f,    #00000000,  #dc322f,  #fdf6e3
rofi.color-active: #00000000,  #268bd2,    #00000000,  #268bd2,  #fdf6e3

!                'background',  'border', 'separator'
rofi.color-window: #c0000000, #c0000000,   #00000000
