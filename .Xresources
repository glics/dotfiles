! term colors
! special
*.foreground:  #00e5ff
*.background:  #212121
*.cursorColor: #00e5ff

! black
*.color0:      #673ab7
*.color8:      #673ab7

! red
*.color1:      #e91e63
*.color9:      #e91e63

! green
*.color2:      #00e676
*.color10:     #00e676

! yellow
*.color3:      #ffc400
*.color11:     #ffc400

! blue
*.color4:      #00b0ff
*.color12:     #00b0ff

! magenta
*.color5:      #9575cd
*.color13:     #9575cd

! cyan
*.color6:      #1de9b6
*.color14:     #1de9b6

! white
*.color7:      #ffffff
*.color15:     #ffffff

!urxvt stuff
urxvt.scrollBar:      false
urxvt*depth:          32
urxvt.background:     [70]#000000
urxvt.internalBorder: 20
urxvt.font:           xft:Knack Nerd Font:pixelsize=13
!urxvt.letterspace:    -1

!rofi
rofi.location: 2
rofi.width:    100
rofi.padding:  30
rofi.eh:       2 
rofi.lines:    5
rofi.font:     Roboto Slab 13

! State:                'bg',     'fg',      'bgalt',   'hlbg',   'hlfg'
rofi.color-normal: #00000000,  #ffffff,    #00000000,  #e91e63,  #ffffff
rofi.color-urgent: #00000000,  #dc322f,    #00000000,  #dc322f,  #fdf6e3
rofi.color-active: #00000000,  #268bd2,    #00000000,  #268bd2,  #fdf6e3

!                'background',  'border', 'separator'
rofi.color-window: #70000000, #40000000,   #00000000
